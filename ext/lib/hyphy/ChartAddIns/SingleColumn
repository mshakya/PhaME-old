							   {""},
							   {""},
							   {""},
							   {""},
							   {""},
							   {"0"}},
							   "SCREEN_WIDTH-60;SCREEN_HEIGHT-50;30;50");
}
else
{
	NON_EMPTY_SELECTION = -1;
}

